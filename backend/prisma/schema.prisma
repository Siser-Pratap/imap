datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Account {
  id         Int      @id @default(autoincrement())
  name       String
  host       String
  port       Int
  secure     Boolean  @default(true)
  username   String
  password   String
  createdAt  DateTime @default(now())
  enabled    Boolean  @default(true)
}
